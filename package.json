{
  "name": "openscum",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "build:worker": "npm run build && npx --yes @opennextjs/cloudflare@latest build",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "npm run build:worker && npm run dev:worker",
    "deploy:worker": "npm run build:worker && wrangler deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:seed:types": "npx tsx prisma/seed-types.ts && npx tsx prisma/seed-outcome-types.ts",
    "db:seed:reports": "npx tsx prisma/seed-reports.ts ",
    "test": "jest --watchAll",
    "test:ci": "NODE_ENV=test jest --ci --passWithNoTests --coverage",
    "postinstall": "prisma generate"
  },
